{
  "manifest_version": 3,
  "name": "Spelling Bean",
  "description": "Make Spelling Bee more fun",
  "version": "1.0",
  "action": {
    "default_popup": "pages/popup.html",
    "default_icon": "bee.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/load.js"],
      "matches": ["https://www.nytimes.com/puzzles/spelling-bee*"]
    }
  ]
}
